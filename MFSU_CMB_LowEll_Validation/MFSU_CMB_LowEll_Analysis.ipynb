d_f_boots = []
for _ in range(500):
    idx = np.random.choice(len(ell), len(ell), replace=True)
    ell_b, cl_b = ell[idx], cl_obs[idx]
    res = minimize(cost, x0=[A_best, d_f_best], args=(ell_b, cl_b))
    d_f_boots.append(res.x[1])
d_f_err = np.std(d_f_boots) * 1.96
